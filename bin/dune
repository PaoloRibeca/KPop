(executables
 (public_names KPopCount KPopCountDB kPopTwist KPopTwistDB)
 (names KPopCount KPopCountDB kPopTwist KPopTwistDB)
 (libraries str unix BiOCamLib KPop)
 (ocamlopt_flags (:standard -O3 -ccopt -O3 -ccopt -static)))

